{
  "name": "@telegraf/client",
  "version": "0.0.0",
  "description": "Minimal Telegram Bot API client",
  "type": "module",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "src/**/*.ts",
    "lib/"
  ],
  "scripts": {
    "prepare": "tsc",
    "clean": "git clean -fXde '!node_modules/'",
    "fmt": "deno fmt --ignore=lib/,node_modules/",
    "lint": "deno lint --ignore=lib/,node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telegraf/client.git"
  },
  "homepage": "https://fromdeno.org",
  "keywords": [
    "telegram bot api client"
  ],
  "author": "Wojciech Pawlik <woj.pawlik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/telegraf/client/issues"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "fetch-blob": "^3.1.2",
    "formdata-polyfill": "^4.0.6",
    "node-fetch": "^3.0.0-beta.10"
  },
  "peerDependencies": {
    "typegram": "^3.4.3"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  }
}
