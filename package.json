{
  "name": "@telegraf/client",
  "version": "0.1.1",
  "description": "Minimal library for performing Telegram Bot API calls",
  "type": "commonjs",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "src/**/*.ts",
    "lib/"
  ],
  "scripts": {
    "prepare": "tsc",
    "clean": "git clean -fXde '!node_modules/'",
    "fmt": "deno fmt --ignore=lib/,node_modules/",
    "lint": "deno lint --ignore=lib/,node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telegraf/client.git"
  },
  "homepage": "https://fromdeno.org",
  "keywords": [
    "telegram bot api client"
  ],
  "author": "Wojciech Pawlik <woj.pawlik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/telegraf/client/issues"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "fetch-blob": "^3.1.3",
    "formdata-polyfill": "^4.0.10",
    "node-fetch": "^3.1.0"
  },
  "peerDependencies": {
    "@grammyjs/types": "^2.2.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "typescript": "^4.6.0-dev.20211114"
  }
}
